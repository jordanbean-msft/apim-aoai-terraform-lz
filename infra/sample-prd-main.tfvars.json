    ],
    "private_endpoint_subnet_name": "private-endpoints",
    "private_endpoint_subnet_address_prefixes": [
      "10.1.2.64/26"
    ]
  },
  "openai": {
    "pools": [
      {
        "name": "aoai-default-load-balanced-pool",
