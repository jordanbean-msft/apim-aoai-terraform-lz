              {
                "model": {
                  "format": "OpenAI",
                  "name": "text-embedding-3-large",
                  "version": "1"
                },
                "sku": {
                  "name": "GlobalStandard",
                  "capacity": 50
                },
                "rai_policy_name": "Microsoft.DefaultV2"
              }
            ]
          }
        ]
      }
    ]
  },
  "cosmos_db": {
    "document_time_to_live": 2592000,
    "max_throughput": 4000,
    "zone_redundant": false
  },
  "redis": {
    "shouldDeployRedis": false,
    "capacity": 2,
    "sku_name": "Enterprise_E5",
    "zones": []
  },
  "storage_account": {
    "tier": "Standard",
    "replication_type": "LRS"
  },
  "event_hub": {
    "namespace_sku": "Standard",
    "capacity": 1,
    "maximum_throughput_units": 40,
    "partition_count": 4,
    "message_retention": 1
  },
  "function_app": {
    "sku_name": "FC1",
    "zone_balancing_enabled": false
  }
