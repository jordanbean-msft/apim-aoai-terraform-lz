            "priority": 1,
            "deployments": [
              {
                "model": {
                  "format": "OpenAI",
                  "name": "gpt-4o",
                  "version": "2024-11-20"
                },
                "sku": {
                  "name": "GlobalStandard",
                  "capacity": 100
                },
                "rai_policy_name": "Microsoft.DefaultV2"
              },
              {
                "model": {
                  "format": "OpenAI",
                  "name": "text-embedding-3-large",
                  "version": "1"
                },
                "sku": {
                  "name": "GlobalStandard",
                  "capacity": 50
                },
                "rai_policy_name": "Microsoft.DefaultV2"
              }
            ]
          },
          {
            "name_suffix": "paygo",
            "kind": "OpenAI",
            "sku_name": "S0",
            "location": "eastus2",
            "priority": 2,
            "deployments": [
              {
                "model": {
                  "format": "OpenAI",
                  "name": "gpt-4o",
                  "version": "2024-11-20"
